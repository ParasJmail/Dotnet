@{
	ViewData["Title"] = "Home Page";
	// var emp = ViewData["myEmployee"] as Employee;
	// var emp = ViewBag.myEmployee as Employee;
	// var emp = TempData["myEmployee"] as Employee;
	// var emps = ViewData["myEmployee"] as List<Employee>;
	var emps = ViewBag.myEmployee;


}

<div class="text-center">
	<h1 class="display-4">Welcome</h1>
	<p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>


@{
	if (emps != null)
	{
		foreach (var item in emps)
		{
			<h2>Id : @item.EmpId</h2>
			<h2>Name : @item.EmpName</h2>
			<h2>Designation : @item.Designation</h2>
			<h2>Salary : @item.Salary</h2>
			<p>----------------------------------</p>
		}
	}
}

